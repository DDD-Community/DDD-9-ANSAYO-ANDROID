<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RatingView">
        <attr name="emphasize" format="boolean" />
        <attr name="currentRating" format="float" />
        <attr name="reviewCount" format="integer" />
    </declare-styleable>

    <declare-styleable name="TextField">
        <attr name="android:hint" />
        <attr name="android:enabled" />
        <attr name="errorFieldMessage" format="string" />
        <attr name="errorFieldVisible" format="boolean" />
        <attr name="android:maxLength" />
    </declare-styleable>

    <declare-styleable name="LabelPrimaryButton">
        <attr name="android:text" />
        <attr name="android:enabled" />
    </declare-styleable>
</resources>
