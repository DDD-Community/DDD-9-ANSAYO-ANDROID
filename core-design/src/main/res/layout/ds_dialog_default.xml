<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="294dp"
    android:layout_height="wrap_content"
    android:paddingBottom="16dp"
    android:background="@drawable/bg_dialog_default">

    <TextView
        android:id="@+id/tv_title"
        style="@style/Text_H3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/N10"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="로그아웃 하시겠습니까?"/>

    <Button
        android:id="@+id/btn_negative"
        style="@style/Text_L1"
        android:layout_width="126dp"
        android:layout_height="48dp"
        android:text="아니요"
        android:textColor="@color/N30"
        android:background="@drawable/button_label_secondary_disabled"
        android:layout_marginTop="47dp"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toBottomOf="@id/tv_title"
        app:layout_constraintStart_toStartOf="parent"/>

    <Button
        android:id="@+id/btn_positive"
        style="@style/Text_L1"
        android:layout_width="126dp"
        android:layout_height="48dp"
        android:text="네"
        android:textColor="@color/N95"
        android:background="@drawable/button_label_primary_enable"
        android:layout_marginTop="47dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toBottomOf="@id/tv_title"
        app:layout_constraintEnd_toEndOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>
</layout>